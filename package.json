		"@babel/preset-typescript": "^7.15.0",
		"@types/jest": "^27.0.1",
		"babel-jest": "^27.2.0",
		"jest": "^27.2.0",
		"ts-node": "^10.2.1"
	},
	"dependencies": {
		"@types/xmldom": "^0.1.31",
